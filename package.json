{
  "private": true,
  "packageManager": "pnpm@6.27.2",
  "engines": {
    "node": ">=14.0",
    "pnpm": ">= 6",
    "npm": ">= 8"
  },
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "start": "node .output/server/index.mjs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0 && pretty-quick --check --branch dev",
    "lint:fix": "eslint . --fix --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0  && pretty-quick --branch dev",
    "lint:stylelint": "stylelint . \"**/*.{vue,less,postcss,css,scss}\"",
    "lint:stylelint:fix": "stylelint . --fix \"**/*.{vue,less,postcss,css,scss}\"",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "commit": "git add . && cz-customizable",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "prepare": "nuxi prepare && husky install"
  },
  "config": {
    "commitizen": {
      "path": [
        "@commitlint/cz-commitlint",
        "cz-customizable"
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "cz-commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": "eslint --fix",
    "*.{*.vue,less,postcss,css,scss}": "stylelint --fix"
  },
  "peerDependencies": {
    "webpack": ">=4.0.0 <5.0.0 || >=5.0.0 <6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@commitlint/cz-commitlint": "^17.2.0",
    "@nuxt/image-edge": "1.0.0-27827328.bc9ddc0",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@unocss/nuxt": "0.48.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vueuse/core": "^9.5.0",
    "@vueuse/nuxt": "^9.5.0",
    "commitizen": "^4.2.5",
    "consola": "*",
    "cz-customizable": "^6.9.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.12.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.7.0",
    "h3": "^0.7.21",
    "husky": "^8.0.2",
    "inquirer": "^8.2.5",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "tsx": "^3.12.0",
    "typescript": "^4.8.4",
    "vite": "4.0.4",
    "vitest": "^0.23.4",
    "vue-tsc": "^1.0.24"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.10",
    "@nuxt/kit": "npm:@nuxt/kit-edge@3.0.1-rc.0-27828744.aa9aec1",
    "@nuxtjs/critters": "^0.4.0",
    "@pinia/nuxt": "^0.4.6",
    "@vue/shared": "^3.2.45",
    "element-plus": "^2.2.28",
    "lodash": "^4.17.21",
    "nuxt": "^3.1.1",
    "swiper": "^8.4.5",
    "unplugin-icons": "^0.15.1",
    "unplugin-vue-components": "^0.22.12",
    "vue": "3.2.45"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
